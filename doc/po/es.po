#. type: <title></title>
#: guide.sgml:4
msgid "APT User's Guide"
msgstr ""

#. type: <author></author>
#: guide.sgml:6 offline.sgml:6
msgid "<name>Jason Gunthorpe </name><email>jgg@debian.org</email>"
msgstr ""

#. type: <version></version>
#: guide.sgml:7
